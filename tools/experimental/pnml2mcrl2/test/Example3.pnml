<?xml version="1.0"?>
<!-- A Petri net that has more than one token, and that deadlocks -->
<pnml>
	<net type="EPNML11" id="example3">
		<place id="pl01">
			<name>
				<text>place01</text>
			</name>
			<type>
				<text>channel</text>
			</type>
			<initialMarking>
				<text>2</text>
			</initialMarking>
		</place>
		<place id="pl02">
			<name>
				<text>place02</text>
			</name>
		</place>
		<transition id="tr01">
			<name>
				<text>trans01</text>
			</name>
			<type>
				<text>AND</text>
			</type>
		</transition>
		<arc id="arc01" source="pl01" target="tr01"/>
		<arc id="arc02" target="pl02" source="tr01"/>
	</net>
</pnml>
