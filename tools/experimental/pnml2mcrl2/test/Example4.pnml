<?xml version="1.0"?>
<!-- A Petri net in which a transition has multiple input-arcs, and another
     transition has multiple output-arcs -->
<pnml>
	<net type="EPNML11" id="example4">
		<place id="pl01">
			<name>
				<text>place01</text>
			</name>
			<type>
				<text>channel</text>
			</type>
			<initialMarking>
				<text>1</text>
			</initialMarking>
		</place>
		<place id="pl02">
			<name>
				<text>place02</text>
			</name>
		</place>
		<place id="pl03">
			<name>
				<text>place03</text>
			</name>
			<type>
				<text>channel</text>
			</type>
		</place>
		<place id="pl04">
			<name>
				<text>place04</text>
			</name>
			<initialMarking>
				<text>0</text>
			</initialMarking>
		</place>
		<transition id="tr01">
			<name>
				<text>trans01</text>
			</name>
			<type>
				<text>AND</text>
			</type>
		</transition>
		<transition id="tr02">
			<name>
				<text>trans02</text>
			</name>
		</transition>
		<arc id="arc01" source="pl01" target="tr01"/>
		<arc id="arc02" target="pl02" source="tr01"/>
		<arc id="arc03" target="pl03" source="tr01"/>
		<arc id="arc04" target="pl04" source="tr01"/>
		<arc id="arc05" source="pl02" target="tr02"/>
		<arc id="arc06" source="pl03" target="tr02"/>
		<arc id="arc07" source="pl04" target="tr02"/>
		<arc id="arc08" target="pl01" source="tr02"/>
	</net>
</pnml>
