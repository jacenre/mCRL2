<?xml version="1.0"?>
<!-- A Petri net in which a place has multiple input-arcs, and another place
     has multiple output-arcs -->
<pnml>
	<net type="EPNML11" id="example5">
		<place id="pl01">
			<name>
				<text>place01</text>
			</name>
			<type>
				<text>channel</text>
			</type>
			<initialMarking>
				<text>1</text>
			</initialMarking>
		</place>
		<place id="pl02">
			<name>
				<text>place02</text>
			</name>
		</place>
		<transition id="tr01">
			<name>
				<text>trans01</text>
			</name>
			<type>
				<text>AND</text>
			</type>
		</transition>
		<transition id="tr02">
			<name>
				<text>trans02</text>
			</name>
		</transition>
		<transition id="tr03">
			<name>
				<text>trans03</text>
			</name>
		</transition>
		<transition id="tr04">
			<name>
				<text>trans04</text>
			</name>
		</transition>
		<arc id="arc01" source="pl01" target="tr01"/>
		<arc id="arc02" source="pl01" target="tr02"/>
		<arc id="arc03" source="pl01" target="tr03"/>
		<arc id="arc04" target="pl02" source="tr01"/>
		<arc id="arc05" target="pl02" source="tr02"/>
		<arc id="arc06" target="pl02" source="tr03"/>
		<arc id="arc07" source="pl02" target="tr04"/>
		<arc id="arc08" target="pl01" source="tr04"/>
	</net>
</pnml>
